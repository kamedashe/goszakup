telegram:
  token: "YOUR_TELEGRAM_TOKEN"
  admin_id: "YOUR_TELEGRAM_ADMIN_ID"

account:
  login: "YOUR_LOGIN"
  password: "YOUR_PASSWORD"
  sign_password: "YOUR_SIGN_PASSWORD" # Пароль от ключа

target:
  url: "https://v3bl.goszakup.gov.kz/ru/user/login"

paths:
  # Имя файла ключа (оно одинаковое и там, и там)
  key_filename: "YOUR_KEY_FILENAME"
  # Путь внутри Докера
  docker_dir: "/goszakup/data"
  # Путь на Windows (Локально)
  local_dir: "d:/goszakup/data"

services:
  ncanode_docker: "http://ncanode:14579"
  ncanode_local: "http://localhost:14579"

data:
  cooks:
    - name: "Иванов Иван"
      diploma_number: "NO12345"
      file_path: "data/test/diploma_1.pdf"
    - name: "Петров Петр"
      diploma_number: "NO67890"
      file_path: "data/test/diploma_2.pdf"